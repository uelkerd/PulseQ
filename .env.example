# Blockchain Configuration
INFURA_URL=https://mainnet.infura.io/v3/YOUR_PROJECT_ID
CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
COORDINATOR_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
PRIVATE_KEY=your_private_key_here

# Optional: Test Network Configuration
# TESTNET_INFURA_URL=https://goerli.infura.io/v3/YOUR_PROJECT_ID
# TESTNET_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
# TESTNET_COORDINATOR_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=test_user
DB_PASSWORD=test_password
DB_NAME=test_db

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# Feature Flags
FEATURE_DISTRIBUTED_TESTING=false
FEATURE_CLOUD_INTEGRATION=false
FEATURE_ADVANCED_CACHING=false
FEATURE_MULTI_ENV_TESTING=false
FEATURE_ENHANCED_MONITORING=false

# Cloud Configuration
CLOUD_PROVIDER=aws
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key

# Monitoring Configuration
MONITORING_ENABLED=true
MONITORING_SERVICE=prometheus
PROMETHEUS_URL=http://localhost:9090

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json

# Test Configuration
TEST_ENVIRONMENT=staging
TEST_TIMEOUT=30
TEST_RETRY_COUNT=3 